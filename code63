// 정수 3개 입력받아 짝수만 출력하기
#include <stdio.h>

int main()
{
    int a, b, c;
    printf("three num\n");

    scanf("%d %d %d", &a, &b, &c);
    
    if(a % 2 == 0)                  // a를 2로 나누었을 때 나머지가 0인 경우
    {
        printf("%d\n", a);
    }
    if(b % 2 == 0)                 // b를 2로 나누었을 때 나머지가 0인 경우
    {
        printf("%d\n", b);
    }
    if(c % 2 == 0)                 // c를 2로 나누었을 때 나머지가 0인 경우
    {
        printf("%d\n", c);
    }
}
