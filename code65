// 정수 1개 입력받아 분석하기 ==================================================================================================> 다시
#include <stdio.h>
int main(void)
{
    int a;
    printf("one number\n");
    scanf("%d", &a);
    if (a % 2 == 0)
    
        printf("even number\n");
    else
    
        printf("odd number\n");
    
    return 0;
}
