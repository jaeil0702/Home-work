// 01. 출력하기
#include <stdio.h> 

int main()
{
    printf("Hello"); 
    return 0;
}

// 02. 출력하기
#include <stdio.h>

int main()
{
    printf("Hello World");
    return 0;
}

// 03. 출력하기
#include <stdio.h>

int main()
{
    printf("Hello\nWorld");
    return 0;
}

// 04. 출력하기
#include <stdio.h>

int main()
{
    printf("'Hello'");
    return 0;
}

// 05. 출력하기
#include <stdio.h>

int main()
{
    printf("\"Hello World\""); // 기호를 출력할땐 백슬래쉬를 사용
    return 0;
}

// 06. 출력하기
#include <stdio.h>

int main()
{
    printf("\"!@#$%^&*()\""); // 기호를 사용할땐 백슬래쉬 사용
    return 0;
}

// 07. 출력하기
#include <stdio.h>

int main()
{
    printf("\"C:\\Download\\hello.cpp\"");
    return 0;
}

// 08. 출력하기
#include <stdio.h>

int main()
{
    printf("\u250C\u2500\u2510\n\u251C\u253C\u2524\n\u2514\u2534\u2518"); // 유니코드를 사용하여 문자 출력
    return 0;
}

// 09. 정수 1개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    int a;

    scanf("%d", &a);
    printf("%d", a);
    return 0;
}

// 10. 문자 1개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    char a;

    scanf("%c", &a);
    printf("%c", a);
    return 0;
}

// 11. 실수 1개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    float a;

    scanf("%f", &a);
    printf("%f", a);
    return 0;
}

// 12. 정수 2개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;

    scanf("%d %d", &a, &b);
    printf("%d %d", a, b);
    return 0;
}

// 13. 문자 2개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    char a;
    char b;

    scanf("%c %c", &a, &b);
    printf("%c %c", b, a);
    return 0;
}

// 14. 실수 입력받아 둘째 자리까지 출력하기
#include <stdio.h>

int main()
{
    float a;

    scanf("%f", &a);
    printf("%.2f", a); // %.2f : float형에서 소수 2번째 자리까지 표시한다(3번째 자리에서 반올림)
    return 0;
}

// 15. 정수 1개 입력받아 3번 출력하기
#include <stdio.h>

int main()
{
    int a;

    scanf("%d", &a);
    printf("%d %d %d", a, a, a);
    return 0;
}

// 16. 시간 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;

    scanf("%d : %d", &a, &b);
    printf("%d : %d", a, b);
    return 0;
}

// 17. 연월일 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;
    int c;

    scanf("%d.%d.%d", &a, &b, &c);
    printf("%04d.%02d.%02d", a, b, c); // %xx로 출력 될 문자의 개수를 정하기
    return 0;
}

// 18. 주민번호 입력받아 형태 바꿔 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;

    scanf("%d - %d", &a, &b);
    printf("%06d - %07d", a, b);
    return 0;
}

// 19. 단어 1개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    char a[15]; // 문자를 저장하기 위해 char[xx]으로 선언

    scanf("%s", &a);
    printf("%s", a);
    return 0;
}

// 20. 문장 1개 입력받아 그대로 출력하기
#include <stdio.h>

int main()
{
    char a[100];
    //scanf("%s", &a); 이렇게 작성했다가 첫 번째 단어만 나왔음
    fgets(a, 99, stdin); // 하나의 라인 자체를 입력 받기 때문에 fgets를 사용
    printf("%s", a);
    return 0;
}

// 21. 실수 1개 입력받아 부분별로 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;

    scanf("%d.%d", &a, &b);
    printf("%d\n%d", a, b);
    return 0;
}

// 22. 단어 1개 입력받아 나누어 출력하기
/*
#include <stdio.h>

int main()
{
    char a;
    char b;
    char c;

    scanf("%c", &a);
    printf("'%c'\n'%c'\n'%c'", a, b, c);
    return 0;
}
*/

#include <stdio.h>

int main()
{
    int i;
    char a[21];

    scanf("%s", &a);
    for(i=0; i<21; i++) // 0부터 21까지 1씩 증가
    {
        if(a[i] == '\0')break; // \0(NULL)을 만나면 break(탈출)한다
        printf("\'%c\'\n", a[i]);
    }
    return 0;
}

// 23. 정수 1개 입력받아 나누어 출력하기
#include <stdio.h>

int main()
{
    int a;
    int b;
    int c;
    int d;
    int e;

    scanf("%1d %1d %1d %1d %1d", &a, &b, &c, &d, &e);
    printf("[%d0000]\n[%d000]\n[%d00]\n[%d0]\n[%d]", a, b, c, d, e);
    return 0;
}

// 24. 시분초 입력받아 분만 출력하기
#include <stdio.h>

int main()
{
    int a, b, c;

    scanf("%d:%d:%d", &a, &b, &c);
    printf("%d", b);
    return 0;
}

// 25. 년월일 입력 받아 형식 바꿔 출력하기
#include <stdio.h>

int main()
{
    int a, b, c;

    scanf("%d.%d.%d", &a, &b, &c);
    printf("%02d-%02d-%04d",c, b, a);
    return 0;
}

// 26. 정수 1개 입력받아 그대로 출력하기2
#include <stdio.h>

int main()
{
    unsigned int a; // unsigned는 자료형 앞에 붙여 부호를 사용하지 않고 양수의 값을 두배 더 사용할 수 있음

    scanf("%u", &a);
    printf("%u", a);
    return 0;
}

// 27. 실수 1개 입력받아 그대로 출력하기2
#include <stdio.h>

int main()
{
    double a; // float형은 주로 4bit의 단정밀도로 실수를 표현하고 double형은 주로 8bit의 배정밀도로 실수표현

    scanf("%lf", &a); // float = %f, double = %lf
    printf("%.11lf", a);
    return 0;
}

// 28. 정수 1개 입력받아 그대로 출력하기3
#include <stdio.h>

int main()
{
    long long int a; // long long int형은 64bit를 사용하는 정수형
    
    scanf("%lld", &a); // long long int = %lld사용
    printf("%lld", a);
    return 0;
}
