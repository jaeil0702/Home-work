// 월 입력받아 계절 출력하기

#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d", &a);
    switch (a)
    {
    case 3:                   //case 뒤에 숫자는 월을 의미
    case 4:
    case 5:
        printf("Spring");
        break;
        
    case 6:
    case 7:
    case 8:
        printf("Summer");
        break;
        
    case 9:
    case 10:
    case 11:
        printf("Fall");
        break; 
        
    case 12:
    case 1:
    case 2:
        printf("Winter");
        break;
    }
    return 0;
}
